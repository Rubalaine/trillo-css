  "bugs": {
    "url": "https://github.com/Rubalaine/natours-css/issues"
  },
  "homepage": "https://github.com/Rubalaine/natours-css#readme",
  "devDependencies": {
    "autoprefixer": "^10.0.0",
    "concat": "^1.0.3",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.0.7"
  }
}
